package checkers;

public class RandomPlayer extends Player {

	
	// Constructor
	public Player(String name, CheckersColor color) {}
	
	// return name of player
	public String getName() {}
	
	// return color of player - BLACK/RED
	public CheckersColor getColor() {}
	
	
	/* return random move from a list of possible moves
	** for a player to choose from in a given state(turn)
	*/
	public Move getMove(GameState state) {}
}
